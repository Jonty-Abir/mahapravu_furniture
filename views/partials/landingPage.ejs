    <div class="section-contact-main contact-container">
      <form action="https://formspree.io/f/xnqrovrv" method="POST">
        <div class="grid-contact grid-two-colum-contact">
          <div>
            <label for="userName"></label>
            <input
              type="text"
              id="userName"
              placeholder="enter your name"
              name="userName"
              required
              autocomplete="off"
            />
          </div>
          <div>
            <label for="email"></label>
            <input
              type="email"
              id="email"
              placeholder="example@gmail.com"
              name="email"
              required
              autocomplete="off"
            />
          </div>
        </div>
        <div>
          <label for="subject"></label>
          <input
            type="text"
            id="subject"
            placeholder="subject"
            name="subject"
            required
            autocomplete="off"
          />
        </div>
        <div>
          <label for="textArea"></label>
          <textarea
            name="textArea"
            id="textArea"
            cols="30"
            rows="10"
            minlength="6"
            placeholder="write message!"
            required
          ></textarea>
        </div>
        <div>
          <label for="submit"></label>
          <input
            type="submit"
            class="btn-contact"
            name="submit"
            style="width: 15rem"
            id="submit"
            value="Send Message"
          />
        </div>
      </form>
    </div>
  </div>
</section>

